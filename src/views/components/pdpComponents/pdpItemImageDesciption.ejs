<div class="item-description-wrapper">
    <div class="row">
        <div class="col-sm-2" style="padding: 0">
            <div class="swiper-container">
                <div class="swiper-wrapper">
                    <% _.each(data.image_groups.filter(pic => pic.view_type === 'small'), picture => {%>
                        <div class="swiper-slide">
                            <img style="height: 108px; width: 108px" src="/images/<%= picture.images[0].link%>" alt="">
                        </div>
                    <%});%>
                </div>
            </div>
        </div>
        <div class="col-sm-10">
            <img style="min-height: 615px; min-width: 615px;" src="/images/<%=imageLink%>"  alt="">
        </div>
    </div>
</div>
